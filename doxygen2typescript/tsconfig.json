{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": ["esnext", "dom"], 
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "./dist", 
    "rootDir": ".",
    "declaration": true,
    // "allowJs": false,
    // "checkJs": false,
     "plugins": [{
        "name": "typescript-plugin-proactive-code-fixes"
    }]
  },
  "include": ["src", "test"],
  "exclude": ["node_modules", "dist", "static", "docs"]
}
