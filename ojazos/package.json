{
  "name": "ojazos",
  "version": "0.0.1",
  "description": "opencv.js proposal for npm package and node.js and CLI API",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist docs",
    "clean-npm": " rm -rf node_modules/ package-lock.json",
    "test": "npm run build && npx ava --serial --concurrency 1 --config ava.config.js",
    "test-js": "npm run build && npx ava --serial --concurrency 1 --config ava.config-js.js",
    "test-browser": "exit 0; npx ava --serial --concurrency 1 test-browser/*Test.ts",
    "format": "npx ts-refactor format \"src/**/*.ts*\" \"test/**/*.ts*\" ./formatCodeSettings.json --tsConfigPath ./tsconfig.json --dontAsk",
    "organizeImports": "npx ts-refactor organizeImports \"src/**/*.ts*\" \"test/**/*.ts*\" ./formatCodeSettings.json --tsConfigPath ./tsconfig.json --dontAsk ",
    "lint": " npm run organizeImports && npm run format",
    "prepare": "npm run build ",
    "copy": "cp -r static/opencv.js dist/src",
    "build": "npm run clean && npx tsc&& npm run copy",
    "all": "npm run clean && npm run lint && npm run build && npm test && npm run test-js && npm run test-browser"
  },
  "keywords": [
    "typescript",
    "opencv.js",
    "opencv"
  ],
  "author": "Sebasti√°n Gurin",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.7.1",
    "ava": "^2.2.0",
    "babel-polyfill": "^6.26.0",
    "jimp": "^0.6.4",
    "ts-node": "^8.3.0",
    "ts-refactor": "0.0.9",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "misc-utils-of-mine-generic": "^0.2.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cancerberoSgx/opencv.js-projects-of-mine.git"
  },
  "bugs": {
    "url": "https://github.com/cancerberoSgx/opencv.js-projects-of-mine"
  }
}
