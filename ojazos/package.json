{
  "name": "ojazos",
  "version": "0.0.1",
  "description": "opencv.js proposal for npm package and node.js and CLI API",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist docs",
    "clean-npm": " rm -rf node_modules/ package-lock.json",
    "test": "npm run build && npx ava --serial --concurrency 1 --config ava.config.js && npm run test-browser",
    "test-js": "npm run build && npx ava --serial --concurrency 1 --config ava.config-js.js",
    "test-browser": "exit 0; npx ava --serial --concurrency 1 test-browser/*Test.ts",
    "format": "npx ts-refactor format \"**/*.ts*\" ../../formatCodeSettings.json --tsConfigPath ./tsconfig.json --dontAsk",
    "prepare": "npm run build",
    "organizeImports": "npx ts-refactor organizeImports \"src/**/*.ts*\"   ../../formatCodeSettings.json --tsConfigPath ./tsconfig.json --dontAsk ",
    "lint": "npm run organizeImports && npm run format",
    "build": "npm run clean && npx tsc ",
    "all": "npm run clean && npm run lint && npm run build"
  },
  "keywords": [
    "typescript",
    "opencv.js",
    "opencv"
  ],
  "author": "Sebasti√°n Gurin",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.7.1",
    "ava": "^2.2.0",
    "ts-refactor": "0.0.9",
    "typescript": "^3.5.3"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/cancerberoSgx/opencv.js-projects-of-mine.git"
  },
  "bugs": {
    "url": "https://github.com/cancerberoSgx/opencv.js-projects-of-mine"
  }
}
